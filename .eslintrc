{
  "root": true,
  "extends": [
    "airbnb-typescript/base"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "@typescript-eslint/no-explicit-any": 0,
    "max-len": 0,
    "max-classes-per-file": 0,
    "class-methods-use-this": 0,
    "import/prefer-default-export": 0,
    "import/no-cycle": 0,
    "@typescript-eslint/return-await": 0
  }
}
